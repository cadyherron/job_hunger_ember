<!-- app/templates/dashboard.hbs -->
<h4>Occidential Petroleum</h4>


<div class="row col-md-10">
    <div class="panel-heading">
        <h5 class="panel-title">Suggestions</h5>
    </div>

    <div class="panel panel-default">
      {{#each model.suggestions as |suggestion| }}
        <div class="panel-body dashboard-suggestion">
        {{ suggestion }}
        </div>
      {{/each }}
      <br>
    </div>  
</div>

<div class="row col-md-5">
    <div class="panel-heading">
        <h5 class="panel-title">Resources</h5>
    </div>

    <div class="panel panel-default">
      {{#each model.resources as |resource| }}
        <div class="panel-body dashboard-resource">
        {{ resource }}
        </div>
      {{/each }}
      <br>
    </div>  
</div>





<div class="row col-md-5">
    <div class="panel-heading">
        <h5 class="panel-title">notes</h5>
    </div>

    <div class="panel panel-default">
      <textarea></textarea>
    </div>  
</div>

{{outlet}}
